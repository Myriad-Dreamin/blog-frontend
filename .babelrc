{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "plugins": [
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ],
        "transform-vue-jsx",
        "transform-runtime",
        "transform-object-rest-spread",
        [
            "prismjs",
            {
                "languages": ["html", "css", "haskell", "javascript", "python", "cpp", "c", "php", "go", "shell", "bash", "nginx", "sql"],
                "theme": "okaidia"
            }
        ]
    ]
}
